<% if(errors != "") {%>
    <% errors.forEach((item) => { %>
        <script> openRegisterModal();</script>
        <div class="error alert alert-danger"><%- item %></div>
   <% }) %>
<% } %>


<% if(success != "") {%>
    <% success.forEach((item) => { %>
        <script> openLoginModal();</script>
        <div class="success alert alert-success"><%- item %></div>
   <% }) %>
<% } %>